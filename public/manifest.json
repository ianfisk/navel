{
	"manifest_version": 2,
	"name": "Navel",
	"version": "0.0.1",
	"description": "A simple navigation extension for chrome.",
	"browser_action": {
		"default_icon": "icon.png"
	},

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},

	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"css": ["styles.css"]
	}],

	"permissions": [
		"tabs"
	]
}
